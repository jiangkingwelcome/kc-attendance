<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.kc.dingtalk.mapper.DtTravelMapper">
    <resultMap type="DtTravel" id="DtTravelResult">
        <result property="id" column="id"/>
        <result property="employeeId" column="employee_id"/>
        <result property="name" column="name"/>
        <result property="batchNo" column="batch_no"/>
        <result property="businessCode" column="business_code"/>
        <result property="version" column="version"/>
        <result property="orderStatus" column="order_status"/>
        <result property="rejectType" column="reject_type"/>
        <result property="formName" column="form_name"/>
        <result property="startTime" column="start_time"/>
        <result property="endTime" column="end_time"/>
        <result property="orderStartTime" column="order_start_time"/>
        <result property="orderEndTime" column="order_end_time"/>
        <result property="duration" column="duration"/>
        <result property="createBy" column="create_by"/>
        <result property="createTime" column="create_time"/>
        <result property="updateBy" column="update_by"/>
        <result property="updateTime" column="update_time"/>
        <result property="delFlag" column="del_flag"/>
    </resultMap>

    <sql id="selectDtTravelVo">
        select id,
               employee_id,
               name,
               batch_no,
               business_code,
               version,
               order_status,
               reject_type,
               form_name,
               start_time,
               end_time,
               order_start_time,
               order_end_time,
               duration,
               create_by,
               create_time,
               update_by,
               update_time,
               del_flag
        from dt_travel
    </sql>

    <select id="selectDtTravelList" parameterType="DtTravel" resultMap="DtTravelResult">
        <include refid="selectDtTravelVo"/>
        <where>
            <if test="employeeId != null  and employeeId != ''">and employee_id = #{employeeId}</if>
            <if test="name != null  and name != ''">and name like concat('%', #{name}, '%')</if>
            <if test="batchNo != null  and batchNo != ''">and batch_no = #{batchNo}</if>
            <if test="businessCode != null  and businessCode != ''">and business_code = #{businessCode}</if>
            <if test="version != null  and version != ''">and version = #{version}</if>
            <if test="orderStatus != null  and orderStatus != ''">and order_status = #{orderStatus}</if>
            <if test="rejectType != null  and rejectType != ''">and reject_type = #{rejectType}</if>
            <if test="formName != null  and formName != ''">and form_name like concat('%', #{formName}, '%')</if>
            <if test="startTime != null ">and start_time = #{startTime}</if>
            <if test="endTime != null ">and end_time = #{endTime}</if>
            <if test="orderStartTime != null ">and order_start_time = #{orderStartTime}</if>
            <if test="orderEndTime != null ">and order_end_time = #{orderEndTime}</if>
            <if test="duration != null  and duration != ''">and duration = #{duration}</if>
        </where>
    </select>

    <select id="selectDtTravelById" parameterType="Long" resultMap="DtTravelResult">
        <include refid="selectDtTravelVo"/>
        where id = #{id}
    </select>

    <insert id="insertDtTravel" parameterType="DtTravel" useGeneratedKeys="true" keyProperty="id">
        insert into dt_travel
        <trim prefix="(" suffix=")" suffixOverrides=",">
            <if test="employeeId != null and employeeId != ''">employee_id,</if>
            <if test="name != null">name,</if>
            <if test="batchNo != null and batchNo != ''">batch_no,</if>
            <if test="businessCode != null and businessCode != ''">business_code,</if>
            <if test="version != null and version != ''">version,</if>
            <if test="orderStatus != null and orderStatus != ''">order_status,</if>
            <if test="rejectType != null and rejectType != ''">reject_type,</if>
            <if test="formName != null">form_name,</if>
            <if test="startTime != null">start_time,</if>
            <if test="endTime != null">end_time,</if>
            <if test="orderStartTime != null">order_start_time,</if>
            <if test="orderEndTime != null">order_end_time,</if>
            <if test="duration != null and duration != ''">duration,</if>
            <if test="createBy != null">create_by,</if>
            <if test="createTime != null">create_time,</if>
            <if test="updateBy != null">update_by,</if>
            <if test="updateTime != null">update_time,</if>
            <if test="delFlag != null and delFlag != ''">del_flag,</if>
        </trim>
        <trim prefix="values (" suffix=")" suffixOverrides=",">
            <if test="employeeId != null and employeeId != ''">#{employeeId},</if>
            <if test="name != null">#{name},</if>
            <if test="batchNo != null and batchNo != ''">#{batchNo},</if>
            <if test="businessCode != null and businessCode != ''">#{businessCode},</if>
            <if test="version != null and version != ''">#{version},</if>
            <if test="orderStatus != null and orderStatus != ''">#{orderStatus},</if>
            <if test="rejectType != null and rejectType != ''">#{rejectType},</if>
            <if test="formName != null">#{formName},</if>
            <if test="startTime != null">#{startTime},</if>
            <if test="endTime != null">#{endTime},</if>
            <if test="orderStartTime != null">#{orderStartTime},</if>
            <if test="orderEndTime != null">#{orderEndTime},</if>
            <if test="duration != null and duration != ''">#{duration},</if>
            <if test="createBy != null">#{createBy},</if>
            <if test="createTime != null">#{createTime},</if>
            <if test="updateBy != null">#{updateBy},</if>
            <if test="updateTime != null">#{updateTime},</if>
            <if test="delFlag != null and delFlag != ''">#{delFlag},</if>
        </trim>
    </insert>

    <update id="updateDtTravel" parameterType="DtTravel">
        update dt_travel
        <trim prefix="SET" suffixOverrides=",">
            <if test="employeeId != null and employeeId != ''">employee_id = #{employeeId},</if>
            <if test="name != null">name = #{name},</if>
            <if test="batchNo != null and batchNo != ''">batch_no = #{batchNo},</if>
            <if test="businessCode != null and businessCode != ''">business_code = #{businessCode},</if>
            <if test="version != null and version != ''">version = #{version},</if>
            <if test="orderStatus != null and orderStatus != ''">order_status = #{orderStatus},</if>
            <if test="rejectType != null and rejectType != ''">reject_type = #{rejectType},</if>
            <if test="formName != null">form_name = #{formName},</if>
            <if test="startTime != null">start_time = #{startTime},</if>
            <if test="endTime != null">end_time = #{endTime},</if>
            <if test="orderStartTime != null">order_start_time = #{orderStartTime},</if>
            <if test="orderEndTime != null">order_end_time = #{orderEndTime},</if>
            <if test="duration != null and duration != ''">duration = #{duration},</if>
            <if test="createBy != null">create_by = #{createBy},</if>
            <if test="createTime != null">create_time = #{createTime},</if>
            <if test="updateBy != null">update_by = #{updateBy},</if>
            <if test="updateTime != null">update_time = #{updateTime},</if>
            <if test="delFlag != null and delFlag != ''">del_flag = #{delFlag},</if>
        </trim>
        where id = #{id}
    </update>

    <delete id="deleteDtTravelById" parameterType="Long">
        delete
        from dt_travel
        where id = #{id}
    </delete>

    <delete id="deleteDtTravelByIds" parameterType="String">
        delete from dt_travel where id in
        <foreach item="id" collection="array" open="(" separator="," close=")">
            #{id}
        </foreach>
    </delete>

    <delete id="deleteByBusinessCodes" parameterType="String">
        delete
        from dt_travel
        where
        <if test="businessCodes != null">
            business_code
            <foreach collection="businessCodes" item="businessCode" separator="," open="in(" close=")">
                #{businessCode}
            </foreach>
        </if>
    </delete>


    <insert id="batchInsert" useGeneratedKeys="true" keyProperty="id">
        insert into
        dt_travel(employee_id, name, batch_no, business_code, version, order_status, reject_type, form_name, start_time,
        end_time,order_start_time,order_end_time, duration, create_by, create_time, del_flag
        )
        values
        <foreach collection="list" item="item" separator=",">
            (
            #{item.employeeId},
            #{item.name},
            #{item.batchNo},
            #{item.businessCode},
            #{item.version},
            #{item.orderStatus},
            #{item.rejectType},
            #{item.formName},
            #{item.startTime},
            #{item.endTime},
            #{item.orderStartTime},
            #{item.orderEndTime},
            #{item.duration},
            #{item.createBy},
            sysdate(),
            #{item.delFlag}
            )
        </foreach>
    </insert>
</mapper>
